# Copy this file to `.env` and fill in the required API keys.
# OpenAI API key (for OpenAI or compatible endpoints)
OPENAI_API_KEY=
# Gemini API key (Google AI Studio)
GEMINI_API_KEY=
# OpenRouter API key (required when using the OpenRouter provider)
OPENROUTER_API_KEY=
# OpenRouter base URL (set when using a custom OpenRouter endpoint)
OPENROUTER_BASE_URL=
# Ollama base URL (preferred; overrides the legacy OLLAMA_HOST when set)
OLLAMA_BASE_URL=
# Ollama host URL (legacy fallback used when OLLAMA_BASE_URL is unset)
OLLAMA_HOST=
