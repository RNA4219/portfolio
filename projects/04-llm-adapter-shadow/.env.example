# Gemini (Google AI Studio) API key
GEMINI_API_KEY=your-gemini-key
# Provider wiring
PRIMARY_PROVIDER=gemini:gemini-2.5-flash
SHADOW_PROVIDER=ollama:gemma3n:e2b
# Ollama endpoint (defaults to http://127.0.0.1:11434 if unset)
OLLAMA_BASE_URL=http://127.0.0.1:11434
# OpenAI API key (OpenAI providerや互換エンドポイントで利用)
OPENAI_API_KEY=sk-your-openai-key
# OpenAI互換エンドポイントを使う場合のみ上書き
OPENAI_BASE_URL=https://api.openai.com/v1
# OpenRouter API key（Anthropic/OpenAI等をOpenRouter経由で呼び出す場合に必要）
OPENROUTER_API_KEY=your-openrouter-key
# セルフホストやリージョン変更時のみ上書き
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
